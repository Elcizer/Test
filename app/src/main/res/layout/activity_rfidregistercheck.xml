<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RfidregistercheckActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_bold"
        android:text="생년월일 "
        android:textColor="#673AB7"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_check_finish"
        app:layout_constraintEnd_toStartOf="@+id/tv_birthday_check"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/tv_rfidnum"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/pretendard_bold"
        android:text="@string/rfid_abc"
        android:textColor="#000000"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pretendard_bold"
        android:text="이름"
        android:textColor="#673AB7"
        android:textSize="40sp"
        app:layout_constraintEnd_toStartOf="@+id/tv_name_check"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/tv_birthday_check"
        android:layout_width="190dp"
        android:layout_height="55dp"
        android:layout_marginEnd="50dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_check_finish"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_name_check" />

    <TextView
        android:id="@+id/tv_name_check"
        android:layout_width="190dp"
        android:layout_height="55dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <android.widget.Button
        android:id="@+id/btn_check_finish"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginBottom="150dp"
        android:background="@drawable/btn_round_edge"
        android:fontFamily="@font/pretendard_bold"
        android:text="확인"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="저장 되었습니다"
        android:fontFamily="@font/pretendard_bold"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_rfidnum" />


</androidx.constraintlayout.widget.ConstraintLayout>